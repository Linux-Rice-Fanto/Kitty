{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#26C6DA",
          "properties": {
            "macos": "mac"
          },
          "style": "plain",
          "template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}}",
          "type": "os"
        },
        {
          "foreground": "#24bdf0",
          "style": "plain",
          "template": " {{ .UserName }}: ",
          "type": "session"
        },
        {
          "foreground": "#fbbe06",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "properties": {
            "branch_icon": "îœ¥ ",
            "fetch_stash_count": true
          },
          "style": "plain",
          "template": " on <#A64CA6>{{ .HEAD }}</> {{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "foreground": "#906cff",
          "style": "powerline",
          "template": "[\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}] ",
           "properties": {
             "display_version": true 
          },
          "type": "python"
        },
        {
          "foreground": "#7FD5EA",
          "style": "powerline",
          "template": "[\ue626 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}] ",
           "properties": {
             "display_version": true 
          },
          "type": "go"
        },
        {
          "foreground": "#76b367",
          "style": "powerline",
          "template": "[\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}] ",
           "properties": {
             "display_version": true 
          },
          "type": "node"
        },
        {
          "foreground": "#f44336",
          "style": "powerline",
          "template": "[\ue791{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}] ",
           "properties": {
             "display_version": true 
          },
          "type": "ruby"
        },
        {
          "foreground": "#FFA500",
          "style": "powerline",
          "template": "[\ue738 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}] ",
          "properties": {
             "display_version": true 
          },
          "type": "java"
        },
        {
          "foreground": "#FFA500",
          "style": "powerline",
          "template": "[\ue738 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}] ",
           "properties": {
             "display_version": true 
          },
          "type": "dotnet"
        },
        {
          "foreground": "#4063D8",
          "style": "powerline",
          "template": " \ue624 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
           "properties": {
             "display_version": true 
          },
          "type": "julia"
        },
        {
          "foreground": "#FFD54F",
          "style": "powerline",
          "template": "\uF48A ",
          "properties": {
            "language_keys": ["markdown"]
          },
          "type": "language"
        },
        {
          "foreground": "#8fce00",
          "style": "plain",
          "template": "\uedfb ",
          "type": "text"
        }
      ],
        "type": "prompt"
    }
  ],
  "version": 3
}
